{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
    "properties": {
      "system": {
        "type": "string"
      },
      "entity": {
        "type": "string"
      },
      "type": {
        "type": "string"
      },
      "entries": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "field": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "previous": {
              "type": ["string", "null", "boolean", "integer", "number"]
            },
            "current": {
              "type": ["string", "null", "boolean", "integer", "number"]
            }
          },
          "required": ["field", "type"]
        }
      }
    },
    "required": ["system", "entity", "type", "entries"]
}